msgid ""
"Examples:\n"
"\tgawk '{ sum += $1 }; END { print sum }' file\n"
"\tgawk -F: '{ print $1 }' /etc/passwd\n"
msgstr ""
"使用例:\n"
"\tgawk '{ sum += $1 }; END { print sum }' 入力ファイル\n"
"\tgawk -F: '{ print $1 }' /etc/passwd\n"

