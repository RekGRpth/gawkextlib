#: array.c:112
#, c-format
msgid "attempt to use function `%s' as an array"
msgstr "関数 `%s' を配列として使用しています。"

#: array.c:115
#, c-format
msgid "attempt to use scalar parameter `%s' as an array"
msgstr "スカラーパラメータ `%s' を配列として使用しています。"

#: array.c:118
#, c-format
msgid "attempt to use scalar `%s' as array"
msgstr "スカラー `%s' を配列として使用しています。"

#: array.c:156
#, c-format
msgid "from %s"
msgstr "%s から"

#: array.c:523
#, c-format
msgid "reference to uninitialized element `%s[\"%s\"]'"
msgstr "初期化されていない要素 `%s[\"%s\"]' を参照しています。"

#: array.c:529
#, c-format
msgid "subscript of array `%s' is null string"
msgstr "配列の添字 `%s' が空列です。"

#: array.c:619
#, c-format
msgid "delete: index `%s' not in array `%s'"
msgstr "delete: 配列 `%2$s' に指標 `%1$s' はありません。"

#: array.c:797
#, c-format
msgid "%s: empty (null)\n"
msgstr "%s: 空 (null)\n"

#: array.c:802
#, c-format
msgid "%s: empty (zero)\n"
msgstr "%s: 空 (zero)\n"

#: array.c:806
#, c-format
msgid "%s: table_size = %d, array_size = %d\n"
msgstr "%s: テーブルサイズ (table_size) = %d, 配列サイズ (array_size) = %d\n"

#: array.c:835
#, c-format
msgid "%s: is parameter\n"
msgstr "%s: はパラメータです。\n"

#: array.c:840
#, c-format
msgid "%s: array_ref to %s\n"
msgstr "%s: %s への配列参照 (array_ref)\n"

#: awkgram.y:256
#, c-format
msgid "%s blocks must have an action part"
msgstr "%s ブロックにはアクション部が必須です。"

#: awkgram.y:259
msgid "each rule must have a pattern or an action part"
msgstr "各ルールにはパターンまたはアクション部が必須です。"

#: awkgram.y:315
#, c-format
msgid "`%s' is a built-in function, it cannot be redefined"
msgstr "`%s' は組込み関数です。再定義できません。"

#: awkgram.y:361
msgid "regexp constant `//' looks like a C++ comment, but is not"
msgstr "正規表現定数 `//' は C++コメントに似ていますが、違います。"

#: awkgram.y:364
#, c-format
msgid "regexp constant `/%s/' looks like a C comment, but is not"
msgstr "正規表現定数 `/%s/' は Cコメントに似ていますが、違います。"

#: awkgram.y:391 awkgram.y:671
msgid "statement may have no effect"
msgstr "命令文には効果が無いかもしれません。"

#: awkgram.y:488 awkgram.y:508
#, c-format
msgid "`%s' used in %s action"
msgstr "`%s' が %s のアクションで使われました。"

#: awkgram.y:501 awkgram.y:504
msgid "`nextfile' is a gawk extension"
msgstr "`nextfile' は gawk 特有の拡張です。"

#: awkgram.y:518
msgid "`return' used outside function context"
msgstr "`return' が関数定義文の外で使われました。"

#: awkgram.y:560
msgid "plain `print' in BEGIN or END rule should probably be `print \"\"'"
msgstr ""
"BEGIN または END ルール内の引数の無い `print' は `print \"\"' だと思われま"
"す。"

#: awkgram.y:573 awkgram.y:580
msgid "`delete array' is a gawk extension"
msgstr "`delete array' は gawk 特有の拡張です。"

#: awkgram.y:588 awkgram.y:595
msgid "`delete(array)' is a non-portable tawk extension"
msgstr "`delete(array)' は tawk 特有の拡張です。他の awk では使用できません。"

#: awkgram.y:639
#, c-format
msgid "duplicate case values in switch body: %s"
msgstr "switch 文の中に重複した case 値が使用されています: %s"

#: awkgram.y:649
msgid "Duplicate `default' detected in switch body"
msgstr "switch 文の中に `default' が重複しています。"

#: awkgram.y:738
msgid "multistage two-way pipelines don't work"
msgstr "多段階で双方向パイプを利用した式は使用できません。"

#: awkgram.y:829
msgid "regular expression on right of assignment"
msgstr "正規表現が代入式の右辺に使用されています。"

#: awkgram.y:852
msgid "regular expression on left of `~' or `!~' operator"
msgstr "`~' や `!~' 演算子の左辺に正規表現が使用されています。"

#: awkgram.y:860
msgid "regular expression on right of comparison"
msgstr "比較式の右辺に正規表現が使用されています。"

#: awkgram.y:927
msgid "non-redirected `getline' undefined inside END action"
msgstr "リダイレクトされていない `getline' は END アクションでは未定義です。"

#: awkgram.y:954
msgid "call of `length' without parentheses is not portable"
msgstr "括弧無しの `length' は他の awk で使用できません。"

#: awkgram.y:957
msgid "call of `length' without parentheses is deprecated by POSIX"
msgstr "POSIX は括弧無しでの `length' の使用を推奨しません。"

#: awkgram.y:1010
msgid "use of non-array as array"
msgstr "配列でないものを配列として使用しています。"

#: awkgram.y:1012
msgid "invalid subscript expression"
msgstr "添字式が不正です。"

#: awkgram.y:1202
msgid "unexpected newline or end of string"
msgstr "予想外の改行または文字列の終端があります。"

#: awkgram.y:1298
msgid "empty program text on command line"
msgstr "コマンド行のプログラム表記が空です。"

#: awkgram.y:1354
#, c-format
msgid "can't open source file `%s' for reading (%s)"
msgstr "ソースファイル `%s' を読み込むために開けません (%s)。"

#: awkgram.y:1452
#, c-format
msgid "can't read sourcefile `%s' (%s)"
msgstr "ソースファイル `%s' を読み込めません (%s)。"

#: awkgram.y:1460
#, c-format
msgid "source file `%s' is empty"
msgstr "ソースファイル `%s' は空です。"

#: awkgram.y:1698 awkgram.y:1820 awkgram.y:1838 awkgram.y:2209 awkgram.y:2343
#: awkgram.y:2396
msgid "source file does not end in newline"
msgstr "ソースファイルが改行で終っていません。"

#: awkgram.y:1760
msgid "unterminated regexp ends with `\\' at end of file"
msgstr "終端されていない正規表現がファイル最後の `\\' で終っています。"

#: awkgram.y:1784
#, c-format
msgid "%s: %d: tawk regex modifier `/.../%c' doesn't work in gawk"
msgstr "%s: %d: tawk の正規表現修飾子 `/.../%c' は gawk で使用できません。"

#: awkgram.y:1788
#, c-format
msgid "tawk regex modifier `/.../%c' doesn't work in gawk"
msgstr "tawk の正規表現修飾子 `/.../%c' は gawk で使用できません。"

#: awkgram.y:1795
msgid "unterminated regexp"
msgstr "正規表現が終端されていません。"

#: awkgram.y:1798
msgid "unterminated regexp at end of file"
msgstr "ファイルの中で正規表現が終端されていません。"

#: awkgram.y:1864
msgid "use of `\\ #...' line continuation is not portable"
msgstr "`\\ #...' 形式の行継続は他の awk で使えません。"

#: awkgram.y:1876
msgid "backslash not last character on line"
msgstr "バックスラッシュが行最後の文字になっていません。"

#: awkgram.y:1921
msgid "POSIX does not allow operator `**='"
msgstr "POSIX は演算子 `**=' を許可しません。"

#: awkgram.y:1923
msgid "old awk does not support operator `**='"
msgstr "演算子 `**=' は古い awk で使えません。"

#: awkgram.y:1932
msgid "POSIX does not allow operator `**'"
msgstr "POSIX では演算子 `**' を許可していません。"

#: awkgram.y:1934
msgid "old awk does not support operator `**'"
msgstr "演算子 `**' は古い awk で使えません。"

#: awkgram.y:1965
msgid "operator `^=' is not supported in old awk"
msgstr "演算子 `^=' は古い awk で使えません。"

#: awkgram.y:1973
msgid "operator `^' is not supported in old awk"
msgstr "演算子 `^' は古い awk で使えません。"

#: awkgram.y:2057 awkgram.y:2072
msgid "unterminated string"
msgstr "文字列が終端されていません。"

#: awkgram.y:2264
msgid "unknown @ directive"
msgstr "@ 指示が不明です。"

#: awkgram.y:2315
msgid "missing filename in @ directive"
msgstr "@ 指示のファイル名がありません。"

#: awkgram.y:2357
#, c-format
msgid "invalid char '%c' in expression"
msgstr "表現の char '%c' は不正です。"

#: awkgram.y:2405
#, c-format
msgid "`%s' is a gawk extension"
msgstr "`%s' は gawk 特有の拡張です。"

#: awkgram.y:2408
#, c-format
msgid "`%s' is a Bell Labs extension"
msgstr "`%s' はベル研究所による拡張です。"

#: awkgram.y:2411
#, c-format
msgid "POSIX does not allow `%s'"
msgstr "POSIX は `%s' を許可しません。"

#: awkgram.y:2415
#, c-format
msgid "`%s' is not supported in old awk"
msgstr "`%s' は古い awk で使用できません。"

#: awkgram.y:2441
msgid "`goto' considered harmful!\n"
msgstr "`goto' は有害です!\n"

#: awkgram.y:2503
#, c-format
msgid "%d is invalid as number of arguments for %s"
msgstr "%d は %s に使える引数の数としては不正です。"

#: awkgram.y:2522 awkgram.y:2525
msgid "match: third argument is a gawk extension"
msgstr "match: 第三引数は gawk 特有の拡張です。"

#: awkgram.y:2538
#, c-format
msgid "%s: string literal as last arg of substitute has no effect"
msgstr "%s: 文字列リテラルを置き換え最後の引数に使用すると効果がありません。"

#: awkgram.y:2541
#, c-format
msgid "%s third parameter is not a changeable object"
msgstr "%s 第三パラメータは可変オブジェクトではありません。"

#: awkgram.y:2568 awkgram.y:2571
msgid "close: second argument is a gawk extension"
msgstr "close: 第二引数は gawk 特有の拡張です。"

#: awkgram.y:2581
msgid "use of dcgettext(_\"...\") is incorrect: remove leading underscore"
msgstr ""
"dcgettext(_\"...\")の使い方が間違っています: 最初のアンダースコアを削除してく"
"ださい。"

#: awkgram.y:2596
msgid "use of dcngettext(_\"...\") is incorrect: remove leading underscore"
msgstr ""
"dcngettext(_\"...\")の使い方が間違っています: 最初のアンダースコアを削除して"
"ください。"

#: awkgram.y:2667
#, c-format
msgid "function `%s': parameter #%d, `%s', duplicates parameter #%d"
msgstr "関数 `%s': パラメータ #%d, `%s' がパラメータ #%d と重複しています。"

#: awkgram.y:2700
#, c-format
msgid "function `%s': parameter `%s' shadows global variable"
msgstr "関数 `%s': パラメータ `%s' が大域変数を覆い隠しています。"

#: awkgram.y:2812
#, c-format
msgid "could not open `%s' for writing (%s)"
msgstr "`%s' を書込み用に開けませんでした (%s)。"

#: awkgram.y:2813 profile.c:93
msgid "sending profile to standard error"
msgstr "プロファイルを標準エラーに送っています。"

#: awkgram.y:2845
#, c-format
msgid "%s: close failed (%s)"
msgstr "%s: 閉じることができません (%s)。"

#: awkgram.y:2966
msgid "shadow_funcs() called twice!"
msgstr "shadow_funcs() を二回呼び出しています!"

#: awkgram.y:2993
msgid "there were shadowed variables."
msgstr "覆い隠された変数がありました。"

#: awkgram.y:3066
#, c-format
msgid "function `%s': can't use function name as parameter name"
msgstr "関数 `%s': 関数名はパラメータ名に使用できません。"

#: awkgram.y:3076
#, c-format
msgid "function name `%s' previously defined"
msgstr "関数名 `%s' は定義済みです。"

#: awkgram.y:3227 awkgram.y:3233
#, c-format
msgid "function `%s' called but never defined"
msgstr "未定義の関数 `%s' を呼び出しました。"

#: awkgram.y:3236
#, c-format
msgid "function `%s' defined but never called"
msgstr "定義された関数 `%s' は一度も呼び出されませんでした。"

#: awkgram.y:3263
#, c-format
msgid "regexp constant for parameter #%d yields boolean value"
msgstr "パラメータ #%d の正規表現定数はブール値を出力します。"

#: awkgram.y:3307
#, c-format
msgid ""
"function `%s' called with space between name and `(',\n"
"or used as a variable or an array"
msgstr ""
"関数名と `(' の間にスペースを入れて関数 `%s' を呼び出しています。\n"
"または、変数か配列として使われています。"

#: builtin.c:145
#, c-format
msgid "%s to \"%s\" failed (%s)"
msgstr "%s から \"%s\" へ出力できません (%s)。"

#: builtin.c:146
msgid "standard output"
msgstr "標準出力"

#: builtin.c:147
msgid "reason unknown"
msgstr "原因不明"

#: builtin.c:160
msgid "exp: received non-numeric argument"
msgstr "exp: 引数が数値ではありません。"

#: builtin.c:166
#, c-format
msgid "exp: argument %g is out of range"
msgstr "exp: 引数 %g が許容範囲を超えています。"

#: builtin.c:224
#, c-format
msgid "fflush: cannot flush: pipe `%s' opened for reading, not writing"
msgstr "fflush: フラッシュ不可: パイプ `%s' は読み込み専用です。"

#: builtin.c:227
#, c-format
msgid "fflush: cannot flush: file `%s' opened for reading, not writing"
msgstr "fflush: フラッシュ不可: ファイル `%s' は読み込み専用です。"

#: builtin.c:239
#, c-format
msgid "fflush: `%s' is not an open file, pipe or co-process"
msgstr ""
"fflush: `%s' はファイルか、パイプか、並行プロセスでなければなりません。"

#: builtin.c:332
msgid "index: received non-string first argument"
msgstr "index: 第一引数が文字列ではありません。"

#: builtin.c:334
msgid "index: received non-string second argument"
msgstr "index: 第二引数が文字列ではありません。"

#: builtin.c:449
msgid "int: received non-numeric argument"
msgstr "int: 引数が数値ではありません。"

#: builtin.c:472
msgid "`length(array)' is a gawk extension"
msgstr "`length(array)' は gawk 特有の拡張です。"

#: builtin.c:481
msgid "length: received non-string argument"
msgstr "length: 引数が文字列ではありません。"

#: builtin.c:506
msgid "log: received non-numeric argument"
msgstr "log: 引数が数値ではありません。"

#: builtin.c:509
#, c-format
msgid "log: received negative argument %g"
msgstr "log: 引数 %g が負の値です。"

#: builtin.c:703 builtin.c:706
msgid "must use `count$' on all formats or none"
msgstr ""
"`count$’は全ての書式指定に適用する、または全てに適用しないのどちらかでしか使"
"えません。"

#: builtin.c:808
msgid "`$' is not permitted in awk formats"
msgstr "`$' は awk では使えません。"

#: builtin.c:814
msgid "arg count with `$' must be > 0"
msgstr "順序指定の `$' に使える数字は正の値です。"

#: builtin.c:816
#, c-format
msgid "arg count %ld greater than total number of supplied arguments"
msgstr "順序指定 %ld が与えられている引数の数を超えています。"

#: builtin.c:818
msgid "`$' not permitted after period in format"
msgstr "`$' は書式指定のピリオド `.' の後に使用できません。"

#: builtin.c:831
msgid "no `$' supplied for positional field width or precision"
msgstr "フィールド幅、または精度の指定子に `$' が与えられていません。"

#: builtin.c:897
msgid "`l' is meaningless in awk formats; ignored"
msgstr "awk の書式指定では `l' は無意味です。無視します。"

#: builtin.c:901
msgid "`l' is not permitted in POSIX awk formats"
msgstr "POSIX 準拠の awk では `l' は使用できません。"

#: builtin.c:912
msgid "`L' is meaningless in awk formats; ignored"
msgstr "awk の書式指定では `L' は無意味です。無視します。"

#: builtin.c:916
msgid "`L' is not permitted in POSIX awk formats"
msgstr "POSIX 準拠の awk では `L' は使用できません。"

#: builtin.c:927
msgid "`h' is meaningless in awk formats; ignored"
msgstr "awk の書式指定では `h' は無意味です。無視します。"

#: builtin.c:931
msgid "`h' is not permitted in POSIX awk formats"
msgstr "POSIX 準拠の awk では `h' は使用できません。"

#: builtin.c:1180
#, c-format
msgid "[s]printf: value %g is out of range for `%%%c' format"
msgstr "[s]printf: %g の値が書式 `%%%c' の許容範囲を超えています。"

#: builtin.c:1255
msgid "not enough arguments to satisfy format string"
msgstr "書式指定するための引数が足りません。"

#: builtin.c:1257
msgid "^ ran out for this one"
msgstr "^ ここから足りません"

#: builtin.c:1262
msgid "[s]printf: format specifier does not have control letter"
msgstr "[s]printf: 書式指定子に制御文字がありません。"

#: builtin.c:1265
msgid "too many arguments supplied for format string"
msgstr "書式指定文字列の引数が多過ぎます。"

#: builtin.c:1339 builtin.c:1342
msgid "printf: no arguments"
msgstr "printf: 引数がありません。"

#: builtin.c:1366
msgid "sqrt: received non-numeric argument"
msgstr "sqrt: 引数が数値ではありません。"

#: builtin.c:1370
#, c-format
msgid "sqrt: called with negative argument %g"
msgstr "sqrt: 引数に負の値 %g を使用しています。"

#: builtin.c:1394
#, c-format
msgid "substr: start index %g is invalid, using 1"
msgstr "substr: 起点指標 %g が不正です。1 を使います。"

#: builtin.c:1399
#, c-format
msgid "substr: non-integer start index %g will be truncated"
msgstr "substr: 起点指標 %g の小数点以下は切り捨てます。"

#: builtin.c:1418
#, c-format
msgid "substr: length %g is not >= 1"
msgstr "substr: 文字数 %g が 1 以上ではありません。"

#: builtin.c:1420
#, c-format
msgid "substr: length %g is not >= 0"
msgstr "substr: 文字数 %g が 0 以上ではありません。"

#: builtin.c:1427
#, c-format
msgid "substr: non-integer length %g will be truncated"
msgstr "substr: 文字数 %g の小数点以下は切り捨てます。"

#: builtin.c:1432
#, c-format
msgid "substr: length %g too big for string indexing, truncating to %g"
msgstr "substr: 文字数 %g は最大値を超えています。%g を使います。"

#: builtin.c:1444
msgid "substr: source string is zero length"
msgstr "substr: 文字列の長さがゼロです。"

#: builtin.c:1460
#, c-format
msgid "substr: start index %g is past end of string"
msgstr "substr: 起点指標 %g が文字列の長さを超えています。"

#: builtin.c:1468
#, c-format
msgid ""
"substr: length %g at start index %g exceeds length of first argument (%lu)"
msgstr ""
"substr: 起点指標 %2$g からの文字数 %1$g は文字列の長さ %3$lu を超えています。"

#: builtin.c:1543
msgid "strftime: received non-string first argument"
msgstr "strftime: 第一引数が文字列ではありません。"

#: builtin.c:1549
msgid "strftime: received empty format string"
msgstr "strftime: 書式指定文字列が空です。"

#: builtin.c:1558
msgid "strftime: received non-numeric second argument"
msgstr "strftime: 第二引数が数値ではありません。"

#: builtin.c:1621
msgid "mktime: received non-string argument"
msgstr "mktime: 引数が文字列ではありません。"

#: builtin.c:1666
msgid "system: received non-string argument"
msgstr "system: 引数が文字列ではありません。"

#: builtin.c:1787 eval.c:2046
#, c-format
msgid "reference to uninitialized field `$%d'"
msgstr "初期化されていないフィールド `$%d' を参照しています。"

#: builtin.c:1892
msgid "tolower: received non-string argument"
msgstr "tolower: 引数が文字列ではありません。"

#: builtin.c:1922
msgid "toupper: received non-string argument"
msgstr "toupper: 引数が文字列ではありません。"

#: builtin.c:1955
msgid "atan2: received non-numeric first argument"
msgstr "atan2: 第一引数が数値ではありません。"

#: builtin.c:1957
msgid "atan2: received non-numeric second argument"
msgstr "atan2: 第二引数が数値ではありません。"

#: builtin.c:1976
msgid "sin: received non-numeric argument"
msgstr "sin: 引数が数値ではありません。"

#: builtin.c:1992
msgid "cos: received non-numeric argument"
msgstr "cos: 引数が数値ではありません。"

#: builtin.c:2042
msgid "srand: received non-numeric argument"
msgstr "srand: 引数が数値ではありません。"

#: builtin.c:2077
msgid "match: third argument is not an array"
msgstr "match: 第三引数が配列名ではありません。"

#: builtin.c:2620
msgid "gensub: third argument of 0 treated as 1"
msgstr "gensub: 第三引数が 0 です。1 を代わりに使用します。"

#: builtin.c:2794
msgid "lshift: received non-numeric first argument"
msgstr "lshift: 第一引数が数値ではありません。"

#: builtin.c:2796
msgid "lshift: received non-numeric second argument"
msgstr "lshift: 第二引数が数値ではありません。"

#: builtin.c:2802
#, c-format
msgid "lshift(%lf, %lf): negative values will give strange results"
msgstr "lshift(%lf, %lf): 負の数値を使用すると異常な結果になります。"

#: builtin.c:2804
#, c-format
msgid "lshift(%lf, %lf): fractional values will be truncated"
msgstr "lshift(%lf, %lf): 小数点以下は切り捨てになります。"

#: builtin.c:2810
#, c-format
msgid "lshift(%lf, %lf): too large shift value will give strange results"
msgstr "lshift(%lf, %lf): シフト値が大き過ぎると異常な結果になります。"

